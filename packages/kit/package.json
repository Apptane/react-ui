{
  "name": "@apptane/react-ui",
  "description": "Apptane React UI framework components kit",
  "license": "Apache-2.0",
  "version": "0.0.0",
  "homepage": "https://github.com/apptane/react-ui/tree/main/packages/kit#readme",
  "author": "Oleg Grodzevich <oleg@apptane.com>",
  "keywords": [
    "apptane",
    "ui",
    "react",
    "components",
    "framework",
    "kit"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apptane/react-ui.git",
    "directory": "packages/kit"
  },
  "bugs": {
    "url": "https://github.com/apptane/react-ui/issues"
  },
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "clean": "rimraf dist",
    "clean-modules": "rimraf node_modules",
    "build": "concurrently pnpm:build:*",
    "lint": "concurrently pnpm:lint:*",
    "build:esm": "cross-env BABEL_ENV=esm babel src --root-mode upward --extensions .ts,.tsx -d dist/esm --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --root-mode upward --extensions .ts,.tsx -d dist/cjs --source-maps",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "lint:src": "eslint src --ext .ts,.tsx --config ../../.eslintrc.yml",
    "lint:types": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {
    "@apptane/react-ui-avatar": "workspace:^0.0.0",
    "@apptane/react-ui-badge": "workspace:^0.0.0",
    "@apptane/react-ui-banner": "workspace:^0.0.0",
    "@apptane/react-ui-behaviors": "workspace:^0.0.0",
    "@apptane/react-ui-bullet": "workspace:^0.0.0",
    "@apptane/react-ui-button": "workspace:^0.0.0",
    "@apptane/react-ui-calendar": "workspace:^0.0.0",
    "@apptane/react-ui-charts": "workspace:^0.0.0",
    "@apptane/react-ui-core": "workspace:^0.0.0",
    "@apptane/react-ui-cue": "workspace:^0.0.0",
    "@apptane/react-ui-dialog": "workspace:^0.0.0",
    "@apptane/react-ui-divider": "workspace:^0.0.0",
    "@apptane/react-ui-form": "workspace:^0.0.0",
    "@apptane/react-ui-gauge": "workspace:^0.0.0",
    "@apptane/react-ui-glyphs": "workspace:^0.0.0",
    "@apptane/react-ui-hooks": "workspace:^0.0.0",
    "@apptane/react-ui-icon": "workspace:^0.0.0",
    "@apptane/react-ui-input": "workspace:^0.0.0",
    "@apptane/react-ui-layout": "workspace:^0.0.0",
    "@apptane/react-ui-media": "workspace:^0.0.0",
    "@apptane/react-ui-menu": "workspace:^0.0.0",
    "@apptane/react-ui-pane": "workspace:^0.0.0",
    "@apptane/react-ui-progress": "workspace:^0.0.0",
    "@apptane/react-ui-scroller": "workspace:^0.0.0",
    "@apptane/react-ui-selector": "workspace:^0.0.0",
    "@apptane/react-ui-side-panel": "workspace:^0.0.0",
    "@apptane/react-ui-sidebar": "workspace:^0.0.0",
    "@apptane/react-ui-skeleton": "workspace:^0.0.0",
    "@apptane/react-ui-spinner": "workspace:^0.0.0",
    "@apptane/react-ui-tableview": "workspace:^0.0.0",
    "@apptane/react-ui-tabs": "workspace:^0.0.0",
    "@apptane/react-ui-tag": "workspace:^0.0.0",
    "@apptane/react-ui-theme": "workspace:^0.0.0",
    "@apptane/react-ui-toaster": "workspace:^0.0.0",
    "@apptane/react-ui-toggle-group": "workspace:^0.0.0",
    "@apptane/react-ui-tooltip": "workspace:^0.0.0",
    "@apptane/react-ui-typography": "workspace:^0.0.0",
    "@apptane/react-ui-virtual-list": "workspace:^0.0.0",
    "@babel/runtime": "^7.15.4"
  },
  "devDependencies": {
    "@types/react": "17.0.19",
    "react": "17.0.2"
  }
}
